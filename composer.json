{
    "name": "alt-three/validator",
    "description": "A validation wrapper for Laravel",
    "keywords": ["validator", "validation", "Validator", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5 || ^6.0 || ^7.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.4 || ^3.0",
        "phpunit/phpunit": "^6.5 || ^7.5 || ^8.4 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Validator\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
